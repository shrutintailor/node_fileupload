<html>
  <head>
<link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css" rel="stylesheet">

<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<script src="//cdn.datatables.net/1.10.7/js/jquery.dataTables.min.js"></script>
<script src="//cdn.datatables.net/plug-ins/1.10.7/integration/bootstrap/3/dataTables.bootstrap.js"></script>
    
  </head>
<script>
    $(document).ready(function() {
    $('#example').dataTable();
} );

</script>
<h1><center>LIST</center></h1><br>
<!-- &nbsp;&nbsp;&nbsp;&nbsp;<button id ="s" >single</button><br><br> -->
<form action="/fileupload/">
    &nbsp;&nbsp;&nbsp;&nbsp;<button>single</button><br><br>
</form>


<form action="/fileuploadm/">
    &nbsp;&nbsp;&nbsp;&nbsp;<button>Multi</button><br><br>
</form>

<body class = "container">
<table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>No</th>
                <th>File</th>
                <th>Extention</th>
                <th>Show</th>
                <th>Delete</th>
            </tr>
        </thead>
 
        <tfoot>
            <tr>
                <th>No</th>
                <th>File</th>
                <th>Extention</th>
                <th>Show</th>
                <th>Delete</th>
            </tr>
        </tfoot>
 
        <tbody>
            <% patharr.forEach((val,index)=>{    
                // console.log(val)
                // console.log(val.exten)
            %> 
            <tr>    <td><% index + 1 %></td>             
                    <td><img src="<%= val.path %>" height="200px" width="200px" alt="no" /></td>
                    <td><%= val.exten %></td>
                    <td><button ><a href="/list/delete/<%= val.path%>">delete</button></td>
                    <td><button ><a href="/list/show/<%= val.path%>">Show</button></td>
                    </tr>
                    <%  return val }) %>
          </tbody>
    
    </table>
 </body>
</html>